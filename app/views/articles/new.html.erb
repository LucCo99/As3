<%= form_for Article.new do |f| %>
  <p><%= f.label :title %>
    <%= f.text_field :title %> </p>
  <p><%= f.label :body %>
    <%= f.text_area :body %></p>
  <p><%= f.label :category %>
    <%= f.text_field :category %></p>
  <%= f.submit %>
<% end %>
<p>
  <%= link_to "Article List", articles_path %>
</p>

